// TEST
MOV AX, 0xE
MOV [VALUEA], AX

MOV AX, 0x6
MOV [VALUEB], AX

MOV AX, '*'
MOV [OPERATOR], AX

// CODE

// FIND OPERATOR
MOV AX, [OPERATOR]

MOV BX, '+'
CMP AX, BX
JEQ ADDVALUES

MOV BX, '*'
CMP AX, BX
JEQ MULTIPLYVALUES

HALT

// OPERATION CODE
ADDVALUES:
MOV AX, [VALUEA]
MOV BX, [VALUEB]
ADD AX, BX
HALT

MULTIPLYVALUES:
MOV AX, [VALUEA]
MOV BX, [VALUEB]
MUL AX, BX
HALT

HALT
VALUEA: DC.W 0x1FFF
VALUEB: DC.W 0x1FFE
OPERATOR: DC.W 0x1FFD